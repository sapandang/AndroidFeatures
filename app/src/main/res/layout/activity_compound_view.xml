<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".compoundview.CompoundViewActivity">

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Button" />

    <skd.app.androidfeatures.compoundview.DropDown
        android:id="@+id/cw1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </skd.app.androidfeatures.compoundview.DropDown>


    <skd.app.androidfeatures.compoundview.DropDown
        android:id="@+id/cw2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </skd.app.androidfeatures.compoundview.DropDown>

    <skd.app.androidfeatures.compoundview.DropDown
        android:id="@+id/cw3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:label="matrix">

    </skd.app.androidfeatures.compoundview.DropDown>

    <skd.app.androidfeatures.compoundview.DatePicker
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:label="datepicker"></skd.app.androidfeatures.compoundview.DatePicker>

</LinearLayout>